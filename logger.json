{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"basic": {
			"format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
			"datefmt": "%Y-%m-%d %H:%M:%S"
		}
	},
	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "basic",
			"stream": "ext://sys.stdout"
		},
		"file": {
			"class": "logging.FileHandler",
			"level": "DEBUG",
			"formatter": "basic",
			"filename": "./logs/debug.log"
		}
	},
	"root": {
		"level": "DEBUG",
		"handlers": ["console", "file"]
	}
}